{
  "name": "@editorjs/cypress-commands",
  "version": "1.0.0-alpha",
  "description": "Set of Cypress commands for Editor.js testing",
  "main": "dist/commands.js",
  "types": "dist/commands.d.ts",
  "repository": "git@github.com:editor-js/cypress-commands.git",
  "author": "Georgy Berezhnoy <gohabereg@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:commands && yarn build:plugin && yarn build:declarations",
    "build:declarations": "tsc -p ./ --emitDeclarationOnly",
    "build:commands": "webpack",
    "build:plugin": "tsc -p ./cypress/plugins",
    "test": "cypress run",
    "lint": "eslint cypress/ --ext .ts"
  },
  "peerDependencies": {
    "@editorjs/editorjs": "^2.22.2",
    "cypress": "7.x || 8.x"
  },
  "devDependencies": {
    "@editorjs/editorjs": "^2.22.2",
    "@types/node": "^16.4.5",
    "cypress": "^8.3.1",
    "eslint": "^7.31.0",
    "eslint-config-codex": "^1.6.2",
    "ts-loader": "^9.2.4",
    "typescript": "^4.3.5",
    "webpack": "^5.47.0",
    "webpack-cli": "^4.7.2"
  }
}
